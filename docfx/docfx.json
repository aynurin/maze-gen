{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": ["src/**/*.cs"]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net472"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appName": "Player\u0027s Worlds: Procedural Maps",
      "_appTitle": "Player\u0027s Worlds: Procedural Maps",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
